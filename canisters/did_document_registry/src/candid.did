type VerificationMethod = record {
  id : text;
  type_ : text;
  public_key_base58 : text;
};

type DIDDocument = record {
  id : text;
  verification_method : vec VerificationMethod;
};

type Result_Text = variant {
  Ok : text;
  Err : text;
};

type Result_DIDDocument = variant {
  Ok : DIDDocument;
  Err : text;
};

service : () -> {
  register_did : (text, DIDDocument) -> (Result_Text);
  get_did_document : (text) -> (Result_DIDDocument) query;
}

