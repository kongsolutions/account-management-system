      "candid": "src/registry/registry.did",
      "metadata": [
        {
          "name": "candid:service"
        }
      ]
    },
    "did_shard": {
      "type": "custom",
      "build": "cargo build --target wasm32-unknown-unknown --package did_shard --release",
